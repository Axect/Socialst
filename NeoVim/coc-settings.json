{
  "texlab.build.executable": "tectonic",
  "texlab.build.args": [
    "-X",
    "compile",
    "%f",
    "--synctex",
    "--keep-logs",
    "--keep-intermediates"
  ],
  "texlab.build.onSave": true,
  "rust-analyzer.cargo.features": "all",
  "rust-analyzer.inlayHints.typeHints.enable": true,
  "markdownlint.config": {
    "default": true,
    "line_length": false
  },
  "snippets.ultisnips.pythonPrompt": false
}
