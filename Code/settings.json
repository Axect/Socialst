    "workbench.colorCustomizations": {
        "activityBar.foreground": "#ff0734",
        "badge.background": "#ff0734",
        "scrollbarSlider.background": "#ff0734",
        "terminal.foreground": "#ff0734",
        "activityBarBadge.background": "#ff0734"
    },
    "window.menuBarVisibility": "toggle",
    "material-icon-theme.showUpdateMessage": false,
    "workbench.colorTheme": "One Dark Pro",
    "git.autofetch": true,
    // Better Align
    "alignment.operatorPadding": "right",
    "alignment.surroundSpace": {
        "colon": [
            0,
            1
        ],
        "assignment": [
            1,
            1
        ],
        "arrow": [
            1,
            1
        ],
        "comment": 2
    },
    "haskell.hlint.executablePath": "/home/kavis/.local/bin/hlint",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "workbench.startupEditor": "newUntitledFile",
    "files.associations": {
        "*.hs": "haskell",
        "*.md": "markdown",
        "*.F": "FortranFreeForm",
        "*.latex": "pdf",
        "*.ladot": "dot"
    },
    "rust.mode": "legacy",
    "rust.cargoHomePath": "/home/kavis/.cargo/",
    "rust.rustup": {
        "toolchain": "nightly-x86_64-unknown-linux-gnu"
    },
    "explorer.confirmDragAndDrop": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "d.servedPath": "/home/kavis/.local/share/code-d/bin/serve-d",
    // fortran
    "fortran.gfortranExecutable": "/usr/bin/gfortran",
    "fortran.includePaths": [
        "/usr/include",
        "/usr"
    ],
    "fortran.symbols": [
        "function",
        "subroutine"
    ],
    "typescript.locale": "ko",
    "explorer.confirmDelete": false,
    "editor.snippetSuggestions": "top",
    "terminal.integrated.rendererType": "dom",
    "[markdown]": {},
    "liveServer.settings.donotVerifyTags": true
}